{
  "name": "aztec-sandbox-vite-starter",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "DEBUG=app:* vite --force",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "install:noir": "curl -L https://raw.githubusercontent.com/noir-lang/noirup/main/install | bash noirup -v 0.17.0-aztec.2",
    "install:sandbox": "docker pull aztecprotocol/aztec-sandbox:0.11.0",
    "start:sandbox": "SANDBOX_VERSION=0.11.0 /bin/bash -c \"$(curl -fsSL 'https://sandbox.aztec.network')\"",
    "compile": "aztec-cli compile src/contracts/** --outdir ../../artifacts --typescript ../../artifacts"
  },
  "dependencies": {
    "@aztec/aztec.js": "^0.11.0",
    "@aztec/circuits.js": "^0.11.0",
    "@aztec/noir-contracts": "^0.11.0",
    "@aztec/types": "^0.9.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "debug": "^4.3.4",
    "jotai": "^2.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/debug": "^4.1.10",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.16",
    "buffer": "5.7.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vite-plugin-static-copy": "^0.17.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
